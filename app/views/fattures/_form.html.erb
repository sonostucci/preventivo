  <% if fatture.errors.any? %>
    <div id="error_explanation">

      

      <div class="alert alert-danger" role="alert">

        <% fatture.errors.each do |error| %>

          <b>
          <svg class="bi flex-shrink-0 me-2" width="24" height="24"><use xlink:href="#exclamation-triangle-fill"/>
          </svg>
          <%= error.full_message %>
          </b><br>
          
        <% end %>
        
      </div>
        
      
    </div>
  <% end %>
