<p id="notice"><%= notice %></p>

<h1>Fattures</h1>

<table>
  <thead>
    <tr>
      <th>Spettabile</th>
      <th>Nomelavoro</th>
      <th>Iva</th>
      <th>Descrizione</th>
      <th>Quantita</th>
      <th>Prezzo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fattures.each do |fatture| %>
      <tr>
        <td><%= fatture.spettabile %></td>
        <td><%= fatture.nomelavoro %></td>
        <td><%= fatture.iva %></td>
        <td><%= fatture.descrizione %></td>
        <td><%= fatture.quantita %></td>
        <td><%= fatture.prezzo %></td>
        <td><%= link_to 'Show', fatture %></td>
        <td><%= link_to 'Edit', edit_fatture_path(fatture) %></td>
        <td><%= link_to 'Destroy', fatture, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fatture', new_fatture_path %>
